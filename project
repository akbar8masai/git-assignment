prime problem
let num=13;
let factor=0;
for(let i=0;i<=num;i++){
if(num%i==0){
factor++}
} 
if (factor ==2){
console.log('yes')}
else{
console.log('no')}




---------------------
palindrome problem
let str="naman"
let bag=""
for(let i=str.length-1;i>=0;i--){
bag+=str[i];
}
if(bag==str){
console.log('yes')}
else{
console.log('no')}
